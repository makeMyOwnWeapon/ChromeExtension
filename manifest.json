{
  "manifest_version": 3,
  "name": "Learn On Air",
  "version": "1.0",
  "description": "It is an application that helps you focus more when learning",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "images/icon-16.png"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["dist/contentScript.bundle.js"],
      "matches": [
        "https://www.inflearn.com/course*"
      ]
    }
  ],
  "background": {
    "service_worker": "dist/serviceWorker.bundle.js"
  },
  "permissions" : ["notifications", "activeTab"]



  


}